{
    "name": "padviser-react",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.18.0",
        "bcrypt": "^3.0.2",
        "body-parser": "^1.18.3",
        "bootstrap": "^4.0.0",
        "camel-case": "^3.0.0",
        "font-awesome": "^4.7.0",
        "http-proxy-middleware": "^0.19.1",
        "jquery": "^3.3.1",
        "jsonwebtoken": "^8.4.0",
        "mongoose": "^5.3.15",
        "node-sass-chokidar": "^1.2.2",
        "npm-run-all": "^4.1.2",
        "popper.js": "^1.14.3",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-google-maps": "^9.4.5",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.1",
        "redux": "3.7.2",
        "redux-saga": "^0.16.2",
        "redux-thunk": "2.2.0",
        "titleize": "^1.0.1"
    },
    "scripts": {
        "build-css": "node-sass-chokidar ./src/app/layout -o ./src/app/layout",
        "watch-css": "npm run build-css && node-sass-chokidar ./src/app/layout -o ./src/app/layout --watch --recursive",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm-run-all build-css build-js",
        "start-js": "react-scripts start",
        "build-js": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "redux-devtools-extension": "^2.13.7"
    }
}
