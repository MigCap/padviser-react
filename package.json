{
    "name": "padviser-react",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "bootstrap": "^4.0.0",
        "jquery": "^3.3.1",
        "node-sass-chokidar": "^1.2.2",
        "npm-run-all": "^4.1.2",
        "popper.js": "^1.14.3",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-scripts": "2.1.1"
    },
    "scripts": {
        "build-css": "node-sass-chokidar ./src/app/layout -o ./src/app/layout",
        "watch-css": "npm run build-css && node-sass-chokidar ./src/app/layout -o ./src/app/layout --watch --recursive",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm-run-all build-css build-js",
        "start-js": "react-scripts start",
        "build-js": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}